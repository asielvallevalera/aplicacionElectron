{
  "name": "aplicacionelectron",
  "productName": "Products Electron App",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "electron src/index.js",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asielvallevalera/aplicacionElectron.git"
  },
  "keywords": [],
  "author": "Ing. Asiel Valle Valera",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/asielvallevalera/aplicacionElectron/issues"
  },
  "homepage": "https://github.com/asielvallevalera/aplicacionElectron#readme",
  "devDependencies": {
    "electron": "^20.1.0"
  },
  "dependencies": {
    "electron-packager": "^16.0.0",
    "electron-reload": "^2.0.0-alpha.1"
  }
}
